  <xs:complexType name="AllTransferList">
    <xs:complexContent>
      <xs:restriction base="soapenc:Array">
        <xs:sequence>
          <xs:element name="AllQueuedTransferStruct" type="tns:AllQueuedTransferStruct" minOccurs="0" maxOccurs="16"/>
